header {
    background-color: #666;
    text-align: center;
    font-size: 25px;
    color: white;
    margin-bottom: 5%;
    display: flex;
    align-items: center;
}
a:link, a:visited {
    background-color: darkslategrey;
    color: white;
    padding: 14px 25px;
    text-align: center;
    text-decoration: none;
    display: inline-block;
    max-width: fit-content;
    margin: 5px;
}
.wrapper {
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
}
li {
    margin: 10px 0;
}